service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if request.time < timestamp.date(2021, 9, 8);
    }
    match /sessions/{sessionId} {
      allow read;
      allow write: if request.auth.uid == resource.data.sellerUid;

      match /slots/{slotId} {
        allow read, write: if request.auth.uid == resource.data.sellerUid;
      }
    }
  }
}
